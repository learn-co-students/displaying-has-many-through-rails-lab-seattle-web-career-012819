<h1>Dr: <%= @doctor.name %></h1>
<h3>Dept: <%= @doctor.department %></h3>

<h3>Appointments:</h3>
<ul>
  <% @doctor.appointments.each do |app| %>
  <li> <%= link_to app.patient.name, patient_path(app.patient) %></li>
  <%= app.appointment_datetime.strftime("%B %e, %Y at %H:%M") %>
  <% end %>
</ul>
